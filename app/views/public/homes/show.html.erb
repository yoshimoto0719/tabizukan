<h1>TimeL詳細画面</h1>

    <h4>投稿者</h4>
    <%= @end_user.last_name %> <%= @end_user.first_name %>
    <h4>画像</h4>
      <%= image_tag @post.image, size: "200x200" %>
    <h4>場所</h4>
      <%= @post.location %>
    <h4>料金</h4>
      ¥<%= @post.cost %>
    <h4>スケジュール</h4>
      <%= @post.day1 %>泊<%= @post.day2 %>日
    <h4>説明</h4>
      <%= @post.description %>